        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LeaderboardActivity"
            android:label="@string/title_activity_leaderboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".StartActivity"
            android:label="@string/title_activity_start"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Inventory" />
        <activity android:name=".ResultsName" />
        <activity android:name=".ItemView" />
        <activity android:name=".Room1" />
        <activity android:name=".Room2" />
        <activity android:name=".Room3" />
        <activity android:name=".Room4" />
        <activity android:name=".Room5" />
        <activity
            android:name=".Instructions"
            android:label="@string/title_activity_instructions"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>